{
  "name": "stella",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "copyViteManifestForDev": "cp -r dist/.vite public",
    "build": "tsc && vite build && npm run copyViteManifestForDev",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "setup-portal-workflows": "cp -r ./node_modules/decent-portal/github-workflows/*.yml .github/workflows",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.78",
    "@mlc-ai/web-runtime": "^0.18.0-dev2",
    "@mlc-ai/web-tokenizers": "^0.1.5",
    "decent-portal": "^2.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.315",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@webgpu/types": "^0.1.60",
    "eslint": "^9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vitest": "^3.1.1"
  }
}
